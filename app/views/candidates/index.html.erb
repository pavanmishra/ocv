<h1>Listing candidates</h1>

<table>
  <tr>
    <th>State code</th>
    <th>State</th>
    <th>Constituency</th>
    <th>Name</th>
    <th>Sex</th>
    <th>Address line 1</th>
    <th>Address line 2</th>
    <th>City town village</th>
    <th>Age</th>
    <th>Category</th>
    <th>Party</th>
    <th>Symbol no</th>
    <th>Postal votes</th>
    <th>Evm votes</th>
    <th>Total votes</th>
  </tr>

<% @candidates.each do |candidate| %>
  <tr>
    <td><%=h candidate.state_code %></td>
    <td><%=h candidate.state %></td>
    <td><%=h candidate.constituency %></td>
    <td><%=h candidate.name %></td>
    <td><%=h candidate.sex %></td>
    <td><%=h candidate.address_line_1 %></td>
    <td><%=h candidate.address_line_2 %></td>
    <td><%=h candidate.city_town_village %></td>
    <td><%=h candidate.age %></td>
    <td><%=h candidate.category %></td>
    <td><%=h candidate.party %></td>
    <td><%=h candidate.symbol_no %></td>
    <td><%=h candidate.postal_votes %></td>
    <td><%=h candidate.evm_votes %></td>
    <td><%=h candidate.total_votes %></td>
    <td><%= link_to 'Show', candidate %></td>
    <td><%= link_to 'Edit', edit_candidate_path(candidate) %></td>
    <td><%= link_to 'Destroy', candidate, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New candidate', new_candidate_path %>